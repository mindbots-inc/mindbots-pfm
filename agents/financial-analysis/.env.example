# Financial Analysis Agent Configuration

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Agent Settings
FINANCIAL_ANALYZER_ANALYSIS_MODEL=gpt-4
FINANCIAL_ANALYZER_ANALYSIS_PERIOD_DAYS=90
FINANCIAL_ANALYZER_ANOMALY_THRESHOLD=2.5
FINANCIAL_ANALYZER_PREDICTION_HORIZON_DAYS=30
FINANCIAL_ANALYZER_MIN_TRANSACTIONS_FOR_ANALYSIS=10

# Report Settings
FINANCIAL_ANALYZER_REPORT_FREQUENCY=weekly
FINANCIAL_ANALYZER_INCLUDE_VISUALIZATIONS=true

# Machine Learning Settings
FINANCIAL_ANALYZER_ML_CONFIDENCE_THRESHOLD=0.8
FINANCIAL_ANALYZER_ENABLE_TREND_ANALYSIS=true
FINANCIAL_ANALYZER_ENABLE_ANOMALY_DETECTION=true

# Redis Cache Configuration
REDIS_URL=redis://localhost:6379/2
FINANCIAL_ANALYZER_CACHE_TTL=7200

# Integration Settings
FINANCIAL_ANALYZER_BANK_ADAPTER_URL=http://localhost:8001
FINANCIAL_ANALYZER_MEMORY_CELL_URL=

# Performance Settings
FINANCIAL_ANALYZER_MAX_CONCURRENT_ANALYSES=5
FINANCIAL_ANALYZER_BATCH_SIZE=1000

# Logging
FINANCIAL_ANALYZER_LOG_LEVEL=INFO