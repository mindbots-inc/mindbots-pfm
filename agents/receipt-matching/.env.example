# Receipt Matching Agent Configuration

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Agent Settings
RECEIPT_MATCHER_OCR_MODEL=gpt-4-vision
RECEIPT_MATCHER_MATCHING_MODEL=gpt-4
RECEIPT_MATCHER_MATCHING_THRESHOLD=0.75
RECEIPT_MATCHER_FUZZY_MATCH_RATIO=0.8
RECEIPT_MATCHER_AMOUNT_TOLERANCE=0.02
RECEIPT_MATCHER_DATE_TOLERANCE_DAYS=3

# OCR Settings
RECEIPT_MATCHER_OCR_SERVICE_URL=
RECEIPT_MATCHER_MAX_CONCURRENT_OCR=5
RECEIPT_MATCHER_MAX_RECEIPT_SIZE_MB=10

# Redis Cache Configuration
REDIS_URL=redis://localhost:6379/1
RECEIPT_MATCHER_CACHE_TTL=86400

# Logging
RECEIPT_MATCHER_LOG_LEVEL=INFO

# Supported File Formats (comma-separated)
# Default: pdf,png,jpg,jpeg,tiff